<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="clqwq.press.easy_restaurant.mapper.MakeMapper">

    <insert id="insertRecord">
        insert into make(did, mid, num) values(#{did}, #{mid}, #{num})
    </insert>


    <delete id="deleteRecord">
        delete from make where did = #{did}
    </delete>

</mapper>