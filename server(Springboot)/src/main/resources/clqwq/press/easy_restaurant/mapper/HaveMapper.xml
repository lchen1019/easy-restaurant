<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="clqwq.press.easy_restaurant.mapper.HaveMapper">

    <insert id="insertRecord">
        insert into have(oid, did, num) values(#{oid}, #{did}, #{num})
    </insert>


    <delete id="deleteRecord">
        delete from have where oid = #{oid}
    </delete>



</mapper>